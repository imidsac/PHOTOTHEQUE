<% title "FORMATS" %>
<%- model_class = Formatphoto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>

<table id="formatphotos" class="table table-striped table-hover table-condensed display">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:dimention) %></th>
      <th><%= model_class.human_attribute_name(:info) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @formatphotos.each do |formatphoto| %>
      <tr>
        <td><%= link_to formatphoto.id, formatphoto_path(formatphoto) %></td>
        <td><%= formatphoto.dimention %></td>
        <td><%= formatphoto.info %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      formatphoto_path(formatphoto), :class => 'btn btn-info btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_formatphoto_path(formatphoto), :class => 'btn btn-primary btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      formatphoto_path(formatphoto),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-danger btn-xs' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_formatphoto_path,
            :class => 'btn btn-primary' %>
