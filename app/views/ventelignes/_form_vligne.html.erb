<%= simple_form_for [@vente, @venteligne], :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :vente_id %>
  <%= f.input :qte %>
  <%= f.input :qtelivre %>
  <%= f.input :montant %>
  <%= f.input :etat %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
  
  </div>
<% end %>
