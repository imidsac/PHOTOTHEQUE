<%- model_class = Venteligne -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:vente_id) %>:</strong></dt>
  <dd><%= @venteligne.vente_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qte) %>:</strong></dt>
  <dd><%= @venteligne.qte %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qtelivre) %>:</strong></dt>
  <dd><%= @venteligne.qtelivre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:montant) %>:</strong></dt>
  <dd><%= @venteligne.montant %></dd>
  <dt><strong><%= model_class.human_attribute_name(:etat) %>:</strong></dt>
  <dd><%= @venteligne.etat %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              ventelignes_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_venteligne_path(@venteligne), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              venteligne_path(@venteligne),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
