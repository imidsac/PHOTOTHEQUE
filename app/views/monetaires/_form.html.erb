<%= simple_form_for @monetaire, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :money %>
  <%= f.input :code_money %>
  <%= f.input :valeur %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                monetaires_path, :class => 'btn' %>
  </div>
<% end %>
