<%= simple_form_for @prestation, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :client_id %>
  <%= f.input :employe_id %>
  <%= f.input :date_perstation %>
  <%= f.input :etat_prestation %>
  <%= f.input :somme %>
  <%= f.input :payee %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                prestations_path, :class => 'btn' %>
  </div>
<% end %>
