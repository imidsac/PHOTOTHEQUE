<h1>Listing prestations</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Employe</th>
      <th>Date perstation</th>
      <th>Etat prestation</th>
      <th>Somme</th>
      <th>Payee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prestations.each do |prestation| %>
      <tr>
        <td><%= prestation.client %></td>
        <td><%= prestation.employe %></td>
        <td><%= prestation.date_perstation %></td>
        <td><%= prestation.etat_prestation %></td>
        <td><%= prestation.somme %></td>
        <td><%= prestation.payee %></td>
        <td><%= link_to 'Show', prestation %></td>
        <td><%= link_to 'Edit', edit_prestation_path(prestation) %></td>
        <td><%= link_to 'Destroy', prestation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prestation', new_prestation_path %>
