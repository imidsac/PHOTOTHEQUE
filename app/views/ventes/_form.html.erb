<%= simple_form_for @vente, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :boutique_id %>
  <%= f.input :client_id %>
  <%= f.input :clients %>
  <%= f.input :date_vente %>
  <%= f.input :etat_vente %>
  <%= f.input :somme %>
  <%= f.input :payee %>
  <%= f.input :type_ve %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                ventes_path, :class => 'btn' %>
  </div>
<% end %>
