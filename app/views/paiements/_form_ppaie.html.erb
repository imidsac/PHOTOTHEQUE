<%= simple_form_for [@prestation, @paiement], :html => { :class => 'form-horizontal' } do |f| %>
<!--
  <%= f.input :prestation_id %>-->
  <% if @prestation.client_id != -1%>
  <%= f.input :client_id %>
  <% end %>
  <%= f.input :datepaiement %>
  <%= f.input :motif %>
  <%= f.input :montant %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-success' %>
    
  </div>
<% end %>
