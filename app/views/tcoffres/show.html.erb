<%- model_class = Tcoffre -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:date_tc) %>:</strong></dt>
  <dd><%= @tcoffre.date_tc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:beneficier) %>:</strong></dt>
  <dd><%= @tcoffre.beneficier %></dd>
  <dt><strong><%= model_class.human_attribute_name(:type_tc) %>:</strong></dt>
  <dd><%= @tcoffre.type_tc %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              tcoffres_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tcoffre_path(@tcoffre), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tcoffre_path(@tcoffre),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
