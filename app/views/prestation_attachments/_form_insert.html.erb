<%= simple_form_for @prestation_attachment, :html => { :multipart => true } do |f| %>
  <%= f.error_notification %>

  <!--
  <div class="form-inputs">
    <%= f.association :prestation %>
  </div>
-->

  
  <div>
    <%= f.file_field :photo, :multiple => true, name: "prestation_attachment[photo][]" %>
  </div>
  

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
