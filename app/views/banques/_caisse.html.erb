<% title "CAISSE" %>

<%- model_class = Banque -%>
<div class="page-header">
  <h3>Caisse</h3>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      
      <th><%= model_class.human_attribute_name(:paiement) %></th>
      <th><%= model_class.human_attribute_name(:font) %></th>
    </tr>
  </thead>
  <tbody>
    <% @banques.each do |banque| %>
      <tr>
        
        <td><%= banque.solde %></td>
        <td><%= banque.font %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

