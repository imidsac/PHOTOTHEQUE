<%- model_class = Achat -%>
<!--<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>-->

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fournisseur_id) %>:</strong></dt>
  <dd><%= @achat.fournisseur_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:date_achat) %>:</strong></dt>
  <dd><%= @achat.date_achat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:type_ac) %>:</strong></dt>
  <dd><%= @achat.type_ac %></dd>
  <!--<dt><strong><%= model_class.human_attribute_name(:somme) %>:</strong></dt>
  <dd><%= @achat.somme %></dd>
  <dt><strong><%= model_class.human_attribute_name(:payee) %>:</strong></dt>
  <dd><%= @achat.payee %></dd>
  <dt><strong><%= model_class.human_attribute_name(:etat_achat) %>:</strong></dt>
  <dd><%= @achat.etat_achat %></dd>-->
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              achats_path, :class => 'btn-xs btn-info'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_achat_path(@achat), :class => 'btn-xs btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              achat_path(@achat),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn-xs btn-danger' %>
</div>
<hr />
<div class="row">
<div class="col-sm-4">
<h4>Nouveau materiel</h4>
<%= render 'alignes/form_aligne'  %>
</div>
<div class="col-sm-8">
<%= render 'alignes/alignes'  %>
</div>
</div>