<%= simple_form_for(@prestationligne) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :prestation %>
    <%= f.input :qte %>
    <%= f.input :qtelivre %>
    <%= f.input :montant %>
    <%= f.input :numero_prise %>
    <%= f.input :type %>
    <%= f.input :etat %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
