<%= simple_form_for @stock, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :cadre_id %>
  <%= f.input :article_id %>
  <%= f.input :stoc %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                stocks_path, :class => 'btn' %>
  </div>
<% end %>
