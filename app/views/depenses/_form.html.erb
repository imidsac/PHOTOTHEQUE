<%= simple_form_for(@depense) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description %>
    <%= f.association :categoriedep %>
    <%= f.input :montant %>
    <%= f.input :etat_dep %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
