<h1>Listing tcaisses</h1>

<table>
  <thead>
    <tr>
      <th>Date tc</th>
      <th>Crencier</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tcaisses.each do |tcaiss| %>
      <tr>
        <td><%= tcaiss.date_tc %></td>
        <td><%= tcaiss.crencier %></td>
        <td><%= tcaiss.type %></td>
        <td><%= link_to 'Show', tcaiss %></td>
        <td><%= link_to 'Edit', edit_tcaiss_path(tcaiss) %></td>
        <td><%= link_to 'Destroy', tcaiss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tcaisse', new_tcaiss_path %>
